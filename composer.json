{
    "name": "jannisrohden/strava-route-organize-backend",
    "description": "Provides an API to fetch Strava routes and organize them in directories by their name",
    "type": "project",
    "authors": [
        {
            "name": "Jannis Rohden"
        }
    ],
    "require": {
        "symfony/http-foundation": "^5.3",
        "nikic/fast-route": "^1.3",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "App\\Main\\": "src/main",
            "App\\Main\\Auth\\": "src/main/Auth",
            "App\\Main\\Routes\\": "src/main/Routes"
        }
    }
}
